    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/sym_action_email" >
    </ImageView>

    <CheckBox
        android:id="@+id/chk1"
