            android:text="rad1" >
        </RadioButton>
    </RadioGroup>
    <ToggleButton
        android:id="@+id/toggleButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textOff="打开"
        android:textOn="关闭" />

    <AnalogClock
        android:id="@+id/analogClock"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
    </AnalogClock>

    <DigitalClock
        android:id="@+id/digitalClock"
        android:layout_width="wrap_content"
